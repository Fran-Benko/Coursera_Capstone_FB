  {
   "cell_type": "code",
   "execution_count": 59,
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.impute import SimpleImputer\n",
    "imputer = SimpleImputer (strategy='mean')\n",
    "age_meanimp = imputer.fit_transform(df[['Age']])\n",
    "#age_meanimp = pd.DataFrame(age_meanimp, columns=['Age_meanimp'])\n",
    "age_meanimp = pd.Series(age_meanimp[:,0], name='Age_meanimp')\n",
    "df['Age_meanimp'] = age_meanimp"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 60,
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.impute import SimpleImputer\n",
    "imputer2 = SimpleImputer (strategy='median')\n",
    "age_medianimp = imputer2.fit_transform(df[['Age']])\n",
    "#age_meanimp = pd.DataFrame(age_meanimp, columns=['Age_meanimp'])\n",
    "age_medianimp = pd.Series(age_medianimp[:,0], name='Age_meanimp')\n",
